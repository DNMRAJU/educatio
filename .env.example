# Copy this file to .env and update with your actual values

# Your friend's API endpoint base URL (without /api/learn)
# Example: http://api.example.com or http://localhost:8000
REACT_APP_API_URL=http://localhost:8000

# Freepik API Key for image generation (required for scenes with image_prompt or visual_prompt)
REACT_APP_FREEPIK_API_KEY=your_freepik_api_key_here

# Add authentication token if required by the API
# REACT_APP_API_TOKEN=your_api_token_here

# Add any other environment variables here
# REACT_APP_API_KEY=your_api_key_here
